{
  "expo": {
    "name": "ZamanPlanlama",
    "slug": "ZamanPlanlama",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./asset/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./asset/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "ZamanPlanlama",
    "assetBundlePatterns": [
      "**/*",
      "assests/*"
    ],
    "packagerOpts": {
      "assetExts": ["db"]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./asset/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./asset/favicon.png"
    }
  }
}
